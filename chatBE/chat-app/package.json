    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^11.0.0",
    "@nestjs/platform-express": "^11.0.6",
    "@nestjs/platform-socket.io": "^11.0.6",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.0.6",
    "@sentry/nestjs": "^9.30.0",
    "@sentry/node": "^9.30.0",
    "@sentry/tracing": "^7.120.3",
