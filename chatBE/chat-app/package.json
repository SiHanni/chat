{
  "name": "chat-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate:prod": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts src/migration/prod/prod-gen",
    "migration:run:prod": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "migration:generate:prod:msg": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts",
    "migration:run:prod:msg": "NODE_ENV=production ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts",
    "migration:generate:dev": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:generate -d src/data-source.ts src/migration/dev/dev-gen",
    "migration:run:dev": "NODE_ENV=development ts-node ./node_modules/typeorm/cli.js migration:run -d src/data-source.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.735.0",
